; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "EquipmentContainer",
"language": "GDScript",
"path": "res://inventory/EquipmentContainer.gd"
}, {
"base": "Node",
"class": "Item",
"language": "GDScript",
"path": "res://inventory/Item.gd"
}, {
"base": "Control",
"class": "ItemButtonDisplay",
"language": "GDScript",
"path": "res://inventory/ItemButtonDisplay.gd"
}, {
"base": "Node2D",
"class": "POI",
"language": "GDScript",
"path": "res://map/POI.gd"
}, {
"base": "Node",
"class": "Quest",
"language": "GDScript",
"path": "res://quest/Quest.gd"
}, {
"base": "Button",
"class": "QuestButton",
"language": "GDScript",
"path": "res://quest/QuestSelectButton.gd"
} ]
_global_script_class_icons={
"EquipmentContainer": "",
"Item": "",
"ItemButtonDisplay": "",
"POI": "",
"Quest": "",
"QuestButton": ""
}

[application]

config/name="West Marches"
run/main_scene="res://Main.tscn"
config/icon="res://assets/icons/icon.png"

[autoload]

ItemDatabase="*res://inventory/ItemDatabase.tscn"
SignalBus="*res://core/SignalBus.gd"
Globals="*res://core/Globals.gd"

[debug]

gdscript/warnings/unused_signal=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/simple-project-timer/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
